                                ] ;
            rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ;
            rdfs:comment "Formally defined as the set of skos:Concept in concept scheme eli:MediaTypeTable."@en ;
            rdfs:label "Format type"@en ;
            owl:versionInfo "created in v1.2"@en .


