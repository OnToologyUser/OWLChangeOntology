        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations defining particularities of the opening of Tenders. Additional Information: The opening of Tenders is the event when Tenders are made accessible for evaluation, it is generally the same date and time for all Tenders.</skos:definition>
        <skos:prefLabel xml:lang="en">Opening term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#OrganisationGroup -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#OrganisationGroup">
