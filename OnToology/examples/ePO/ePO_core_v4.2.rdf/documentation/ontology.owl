     -->


    <rdf:Description rdf:about="http://data.europa.eu/m8g/birthDate">
        <rdfs:comment xml:lang="en">The point in time on which the Person was born.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">date of birth</rdfs:label>
