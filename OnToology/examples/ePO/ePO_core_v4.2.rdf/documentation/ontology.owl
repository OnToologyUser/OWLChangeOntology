    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#QualificationCriterion -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#QualificationCriterion">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ProcurementCriterion"/>
