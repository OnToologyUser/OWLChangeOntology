        <rdfs:label xml:lang="en">sector</rdfs:label>
        <skos:scopeNote xml:lang="en">For example: environment, safety, housing. Note that a single Public Service may relate to multiple sectors. The possible values for this property are provided as a controlled vocabulary. The possible values for this property are described in the controlled vocabulary &lt;a href=&quot; https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/data-theme&quot;&gt;Data theme&lt;/a&gt; Authority Table of the Publications Office.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/sex -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/sex">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/person#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">The organism&apos;s biological sex.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">sex</rdfs:label>
        <skos:scopeNote xml:lang="en">The recommended controlled vocabulary for this property is &lt;a href=&quot;https://op.europa.eu/en/web/eu-vocabularies/concept-scheme/-/resource?uri=http://publications.europa.eu/resource/authority/human-sex&quot;&gt;the sex authority table of the Publications Office.&lt;/a&gt;</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/specialOpeningHoursSpecification -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/specialOpeningHoursSpecification">
        <rdfs:comment xml:lang="en">A time during which the resource is not available.</rdfs:comment>
        <rdfs:label xml:lang="en">availability restriction</rdfs:label>
        <skos:scopeNote xml:lang="en">For a simple description of the time use the description property of TemporalEntity.

For more structured data use the model in Time Ontology, see an &lt;a href=&quot;https://www.w3.org/TR/owl-time/#dtd-vs-dt&quot;&gt;example&lt;/a&gt;.</skos:scopeNote>
        <skos:scopeNote xml:lang="en">Like the Contact Point, a Channel may not be available at a certain time.
&lt;br/&gt;&lt;br/&gt;
For a simple description of the time use the description property of TemporalEntity.
&lt;br/&gt;&lt;br/&gt;
For more structured data use the model in Time Ontology, see an &lt;a href=&quot;https://www.w3.org/TR/owl-time/#dtd-vs-dt&quot;&gt;example&lt;/a&gt;.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/specifiesEvidenceType -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/specifiesEvidenceType">
        <rdfs:domain rdf:resource="http://data.europa.eu/m8g/EvidenceTypeList"/>
        <rdfs:range rdf:resource="http://data.europa.eu/m8g/EvidenceType"/>
        <rdfs:comment xml:lang="en">Evidence Type included in this Evidence Type List.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">specifies evidence type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/supportsConcept -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/supportsConcept">
