        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">validity period</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/validityPeriodConstraint -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/validityPeriodConstraint">
