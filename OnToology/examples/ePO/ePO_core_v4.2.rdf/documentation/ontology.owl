        <skos:definition xml:lang="en">Criterion used in the first stage of procurement.</skos:definition>
        <skos:prefLabel xml:lang="en">Qualification criterion</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Quantity -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Quantity">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A counted number of non-monetary units possibly including fractions.</skos:definition>
        <skos:prefLabel xml:lang="en">Quantity</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#RequestForClarification -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#RequestForClarification">
