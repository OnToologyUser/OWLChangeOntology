        <rdfs:label xml:lang="en">has contracting authority</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/hasCost -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/hasCost">
        <rdfs:comment xml:lang="en">It indicates the costs related to the execution of a Public Service for the citizen or business related to the execution of the particular Public Service.</rdfs:comment>
        <rdfs:label xml:lang="en">has cost</rdfs:label>
        <skos:scopeNote xml:lang="en">Where the cost varies depending on the channel through which the service is accessed, it can be linked to the channel using the If Accessed Through relationship.</skos:scopeNote>
    </owl:ObjectProperty>
    


