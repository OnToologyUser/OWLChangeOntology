    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/m8g/value -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/m8g/value">
        <rdfs:domain rdf:resource="http://data.europa.eu/m8g/SupportedValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">Value for the Information Concept that the Evidence supports.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/m8g/weight -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/m8g/weight">
        <rdfs:domain rdf:resource="http://data.europa.eu/m8g/Criterion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">Relative importance of the Criterion.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">weight</rdfs:label>
        <skos:scopeNote xml:lang="en">The weight must be between 0 and 1. Usually, all Criteria can be integrated within a weighted sum equal to 1.</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/m8g/weightingConsiderationDescription -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/m8g/weightingConsiderationDescription">
        <rdfs:domain rdf:resource="http://data.europa.eu/m8g/Criterion"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Explanation of how the weighting of a Criterion is to be used.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">weighting consideration description</rdfs:label>
        <skos:scopeNote xml:lang="en">This description gives the view of the creator of the Criterion weights on how to interpret and use them during the evaluation process.</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://data.europa.eu/a4g/ontology#AccessTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AccessTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#LotSpecificTerm"/>
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ProcedureSpecificTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations about where and how to access the Procurement Documents.</skos:definition>
        <skos:prefLabel xml:lang="en">Access term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AcquiringCentralPurchasingBody -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AcquiringCentralPurchasingBody">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#CentralPurchasingBody"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Role of an Agent procuring activities conducted on a permanent basis in the form of the acquisition of supplies and/or services intended for other Buyers. Additional Information: In Public Procurement the Role of Acquiring Central Purchasing Body is carried out by a Central Purchasing Body for other Contracting Authorities.</skos:definition>
        <skos:prefLabel xml:lang="en">Acquiring central purchasing body</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AcquiringParty -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AcquiringParty">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AgentInRole"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The Role of an Agent that acts on the buying side of a Procurement Process.</skos:definition>
        <skos:prefLabel xml:lang="en">Acquiring party</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AdHocChannel -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AdHocChannel">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/m8g/Channel"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Web page where tools and devices for electronic communication that are not generally available can be downloaded free of charge.</skos:definition>
        <skos:prefLabel xml:lang="en">Ad hoc channel</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AgentInRole -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AgentInRole">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Relative concept that ties an Agent to a part they play in a given Situational Context. Additional information: The classification of roles is based is based on the degree of involvement into the Procurement Process: - Primary (Procurement) Pole is directly involved in the Procurement Process; - Secondary (Procurement sub-) Role is secondary to the Procurement Process - Tertiary (Related) Role is not involved in the Procurement Process.</skos:definition>
        <skos:prefLabel xml:lang="en">Agent in role</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AuxiliaryParty -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AuxiliaryParty">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AgentInRole"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Role of an Agent who may be mentioned in the information exchanged during the Procurement Process but who does not play an active part in it.</skos:definition>
        <skos:prefLabel xml:lang="en">Auxiliary party</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AwardCriteriaSummary -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AwardCriteriaSummary">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ProcurementCriteriaSummary"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Overview of the Award Criteria.</skos:definition>
        <skos:prefLabel xml:lang="en">Award criteria summary</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AwardCriterion -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AwardCriterion">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ProcurementCriterion"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Criterion that describes a Requirement that the Tender needs to resolve and on which the Tender is evaluated and ranked.</skos:definition>
        <skos:prefLabel xml:lang="en">Award criterion</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AwardDecision -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AwardDecision">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Document"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Resolution of the Buyer as to the result of the Procurement Procedure.</skos:definition>
        <skos:prefLabel xml:lang="en">Award decision</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AwardEvaluationTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AwardEvaluationTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#EvaluationTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations defining particularities of the evaluation of Award Criteria.</skos:definition>
        <skos:prefLabel xml:lang="en">Award evaluation term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AwardOutcome -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AwardOutcome">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContextualProjection"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Result concerning the competition that was attributed by the Awarder.</skos:definition>
        <skos:prefLabel xml:lang="en">Award outcome</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Awarder -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Awarder">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AcquiringParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent that signs the Award Decision. Additional information: BT-08 can be Awarding CBP; BG-03 is the Buyer. But Awarding CPB is not the same as the Buyer; the organisations may differ for these roles. Having the Awarder Role allows us to specify which Organisation is involved in the Award Decision.</skos:definition>
        <skos:prefLabel xml:lang="en">Awarder</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#AwardingCentralPurchasingBody -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#AwardingCentralPurchasingBody">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#CentralPurchasingBody"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Role of an Agent procuring activities conducted on a permanent basis in the form of the award of Public Contracts or the conclusion of Framework Agreements for works, supplies or services intended for other Buyers. Additional Information: In Public Procurement the Role of Awarding Central purchasing Body is carried out by a Central Purchasing Body for other Contracting Authorities.</skos:definition>
        <skos:prefLabel xml:lang="en">Awarding central purchasing body</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#BudgetProvider -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#BudgetProvider">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AcquiringParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A role of an agent whose Budget is used to pay for the Contract.</skos:definition>
        <skos:prefLabel xml:lang="en">Budget provider</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Business -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Business">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/org#Organization"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A private law company registered in a national registry.</skos:definition>
        <skos:prefLabel xml:lang="en">Business</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Buyer -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Buyer">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AcquiringParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent that awards a Contract and/or purchases items. Additional information: In Public Procurement the Role of Buyer is carried out by the following types of Organisation: Contracting Authority, Contracting Entity, a defense Contractor, an international Organisation, or an organisation awarding a Contract subsidized by a Contracting Authority. In Pre-Award, the Buyer generally awards the Contract, however future purchasers may be foreseen. In Post-Award the buyer generally refers to the purchaser of items.</skos:definition>
        <skos:prefLabel xml:lang="en">Buyer</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Candidate -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Candidate">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#OfferingParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The Role of an Agent that has sought an invitation or has been invited to take part in a restricted Procedure, in a competitive Procedure with negotiation, in a negotiated Procedure without prior publication, in a competitive dialogue or in an innovation partnership.</skos:definition>
        <skos:prefLabel xml:lang="en">Candidate</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#CatalogueProvider -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#CatalogueProvider">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#OfferingParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent compiling and supplying a Catalogue. Additional Information: The Catalogue Provider Role is usually played by the Agent that acts as a Seller, or by another Agent that acts on behalf of the Seller.</skos:definition>
        <skos:prefLabel xml:lang="en">Catalogue provider</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#CatalogueReceiver -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#CatalogueReceiver">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AcquiringParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent processing a Catalogue. Additional Information: The Catalogue Receiver may not only receive it but also validate it, process it, etc. The Catalogue Receiver role is usually played by the agent that acts as a Buyer, or by another Agent that acts on behalf of the Buyer.</skos:definition>
        <skos:prefLabel xml:lang="en">Catalogue receiver</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#CentralPurchasingBody -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#CentralPurchasingBody">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Buyer"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Role of an Agent that provides centralised purchasing activities and, possibly, ancillary purchasing activities for other Buyers. Additional Information: In Public Procurement the Role of Central Purchasing Body is carried out by a Contracting Authority for other Contracting Authorities. Contracting Authority procuring activities conducted on a permanent basis, in one of the following forms:[...](b) the award of Public Contracts or the conclusion of Framework Agreements for works, supplies or services intended for Contracting Authorities;</skos:definition>
        <skos:prefLabel xml:lang="en">Central purchasing body</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Certificate -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Certificate">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/m8g/Evidence"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Proof of conformance of the instance of the concept to a defined set of criteria. Additional Information: Certificates provide assurance of credibility, trust, and transparency in the procurement process.</skos:definition>
        <skos:prefLabel xml:lang="en">Certificate</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#CertificationLabel -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#CertificationLabel">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/m8g/InformationConcept"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Concept that stands for a set of norms, expectations, standards and policies. Additional Information: Such standards may relate to social, ethical and quality etc.</skos:definition>
        <skos:prefLabel xml:lang="en">Certification label</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Certifier -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Certifier">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The role of an agent who issues a certificate. Additional Information: The certifier is responsible for verifying and confirming the conformance of the instance of the concept to a defined set of criteria.</skos:definition>
        <skos:prefLabel xml:lang="en">Certifier</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ChangeInformation -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ChangeInformation">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContextDescription"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Alteration to previously provided details.</skos:definition>
        <skos:editorialNote xml:lang="en">Added hasAdditionalInformation to cover for section VII.2 in F14, since we do not have the Corrigendum notice type anymore in eForms.</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Change information</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ConcessionContract -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ConcessionContract">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Contract"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Contract between one or more Buyers and one or more Economic Operators giving the right to the Economic Operators to exploit the rights foreseen in the Contract which may include the receipt of Payments.</skos:definition>
        <skos:prefLabel xml:lang="en">Concession contract</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ConcessionEstimate -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ConcessionEstimate">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Estimate"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Approximation concerning the foreseen Concession Contract.</skos:definition>
        <skos:prefLabel xml:lang="en">Concession estimate</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ContextDescription -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ContextDescription">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A description concerning a Procurement Object or a Notice in a specific Situation Context. Additional Information: The description has its own identity (+I) but is dependent (+D) on the concerned Entity. We say that ContextDescription *concerns* an Entity.</skos:definition>
        <skos:prefLabel xml:lang="en">Context description</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ContextualProjection -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ContextualProjection">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Projection of an Entity and all of its properties that hold in a given Situation Context. Additional Information: The contextual projection does not have its own identity (-I), is dependent (+D) on the described Entity, and is anti-rigid (~R) We say that ContextualProjection *describes* an Entity. Adaptation of ContextSlices pattern encoding a 4D view. http://ontologydesignpatterns.org/wiki/Submissions:Context_Slices</skos:definition>
        <skos:prefLabel xml:lang="en">Contextual projection</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Contract -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Contract">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Document"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A voluntary, deliberate, and legally binding agreement between two or more competent parties.</skos:definition>
        <skos:prefLabel xml:lang="en">Contract</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ContractLotCompletionInformation -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ContractLotCompletionInformation">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContextDescription"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Information related to a given Lot at the end of the procurement.</skos:definition>
        <skos:prefLabel xml:lang="en">Contract lot completion information</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ContractSpecificTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ContractSpecificTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Term"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Gathering class for conditions and stipulations related to a contract in the Post-Award Phase.</skos:definition>
        <skos:prefLabel xml:lang="en">Contract specific term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ContractTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ContractTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContractSpecificTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations defining particularities of the Post Award Phase.</skos:definition>
        <skos:prefLabel xml:lang="en">Contract term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Contractor -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Contractor">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#OfferingParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The Role of an Agent that has signed a Contract with a Buyer.</skos:definition>
        <skos:prefLabel xml:lang="en">Contractor</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#DesignContestRegimeTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#DesignContestRegimeTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#LotSpecificTerm"/>
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ProcedureSpecificTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations defining particularities of a Design Contest.</skos:definition>
        <skos:prefLabel xml:lang="en">Design contest regime term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#DirectAwardTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#DirectAwardTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ProcedureSpecificTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations defining particularities of the award of a Contract without prior publication. Addtional Information: Direct award may refer to a previous Procedure and/or specific Lot(s). It may also refer to other justified situations whereby there is no previous Procedure. See Directive 2014/24/EU Article 32.</skos:definition>
        <skos:prefLabel xml:lang="en">Direct award term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#DirectContract -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#DirectContract">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Contract"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Contract resulting from Lot Award Outcome. Additional information: A Framework Agreement is not a Direct Contract.</skos:definition>
        <skos:prefLabel xml:lang="en">Direct contract</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Document -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Document">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A set of interrelated Business Information representing the Business facts and associated metadata. The information may be conveyed in any language, medium or form, including textual, numerical, graphic, cartographic, audio-visual forms, etc.</skos:definition>
        <skos:prefLabel xml:lang="en">Document</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Duration -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Duration">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#Duration"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The length of time in which a concept occurs.</skos:definition>
        <skos:prefLabel xml:lang="en">Duration</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#DynamicPurchaseSystemTechnique -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#DynamicPurchaseSystemTechnique">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Technique"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Technique that allows the selection of Candidates throughout the Procedure via the Qualification Criteria, followed by individual Mini-Competitions for the Award of Purchase Contracts.</skos:definition>
        <skos:prefLabel xml:lang="en">Dynamic purchase system technique</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#EAuctionTechnique -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#EAuctionTechnique">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Technique"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A repetitive Technique in which new Prices, revised downwards, and/or new values concerning certain elements of Tenders are bid on-line.</skos:definition>
        <skos:prefLabel xml:lang="en">E auction technique</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#EconomicStandingSummary -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#EconomicStandingSummary">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#SelectionCriteriaSummary"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Overview of the financial viability of the economic operator.</skos:definition>
        <skos:prefLabel xml:lang="en">Economic standing summary</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ElectronicSignature -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ElectronicSignature">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Data in electronic form which is attached to or logically associated with other data in electronic form and which is used by the signatory to sign. Additional information: The definition comes from REGULATION (EU) No 910/2014.</skos:definition>
        <skos:prefLabel xml:lang="en">Electronic signature</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#EmploymentInformationProvider -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#EmploymentInformationProvider">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent responsible for providing information concerning the general regulatory framework for employment protection and working conditions.</skos:definition>
        <skos:prefLabel xml:lang="en">Employment information provider</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#EnvironmentalProtectionInformationProvider -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#EnvironmentalProtectionInformationProvider">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent responsible for providing information concerning the general regulatory framework for Environmental Protection.</skos:definition>
        <skos:prefLabel xml:lang="en">Environmental protection information provider</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Estimate -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Estimate">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContextDescription"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">An approximate calculation or a judgement of the value, number, Quantity or extent of something.</skos:definition>
        <skos:prefLabel xml:lang="en">Estimate</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#EvaluationTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#EvaluationTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#LotSpecificTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations defining particularities of the Tender evaluation.</skos:definition>
        <skos:prefLabel xml:lang="en">Evaluation term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ExclusionGround -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ExclusionGround">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#QualificationCriterion"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Criterion that describes a legal Requirement to be met by the Economic Operator to be a Candidate in the Procurement.</skos:definition>
        <skos:prefLabel xml:lang="en">Exclusion ground</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ExclusionGroundsSummary -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ExclusionGroundsSummary">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#QualificationCriteriaSummary"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Overview of the Exclusion Grounds.</skos:definition>
        <skos:prefLabel xml:lang="en">Exclusion grounds summary</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ExpressionOfInterest -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ExpressionOfInterest">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Document"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Document presenting an Economic Operator&apos;s Request to be considered for Procedures covering a specific domain.</skos:definition>
        <skos:prefLabel xml:lang="en">Expression of interest</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#FrameworkAgreement -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#FrameworkAgreement">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Contract"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">An agreement between one or more Contracting Authorities and one or more Economic Operators. Additional Information: When a Framework Agreement is concerned, the FA will result from LotAwardOutcome which describes a Lot which specifies the both the Qualification and Award Criteria.</skos:definition>
        <skos:prefLabel xml:lang="en">Framework agreement</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#FrameworkAgreementTechnique -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#FrameworkAgreementTechnique">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Technique"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Technique that establishes the terms governing Contracts to be awarded during a given Period, in particular with regard to Price and, where appropriate, the Quantity envisaged.</skos:definition>
        <skos:prefLabel xml:lang="en">Framework agreement technique</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#FrameworkAgreementTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#FrameworkAgreementTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContractSpecificTerm"/>
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#LotSpecificTerm"/>
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ProcedureSpecificTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations defining particularities in a Framework Agreement.</skos:definition>
        <skos:prefLabel xml:lang="en">Framework agreement term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Fund -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Fund">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A financial resource used to support the Procurement. Additional Information: In the context of EU, Funds can be divided into programmes, actions and projects. Examples of EU funds are: the European Structural and Investment Funds, European Social Fund (ESF), the Connecting Europe Facility (CEF) programme, or the ISA2 programme and its actions (e.g. Action 2016.05 European Public Procurement Initiative, which supports the eProcurement Ontology under sub-action 3). Funds may change between the lot and the contract, for example in the case of an emergency crisis, a contract may be financed by a budget that was not foreseen in the call.</skos:definition>
        <skos:prefLabel xml:lang="en">Fund</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#GreenProcurement -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#GreenProcurement">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#StrategicProcurement"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Approach whereby Buyers seek to procure with a reduced Environmental Impact. Additional Information: The approach may apply to the complete life cycle. The reduced Environmental Impact is in comparison to goods, services and works with the same primary function that would otherwise be procured. Tightly related are article 68 - Life-cycle costing and article 67 - most economically advantageous Tender (see GPP handbook) https://ec.europa.eu/environment/gpp/pdf/Buying-Green-Handbook-3rd-Edition.pdf An instance of the class GreenProcurement is represented in eForms with the code &quot;env-imp&quot; defined in the codelist Strategic-Procurement.Paragraphs) https://ec.europa.eu/environment/gpp/pdf/Buying-Green-Handbook-3rd-Edition.pdf</skos:definition>
        <skos:prefLabel xml:lang="en">Green procurement</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#IndefiniteDuration -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#IndefiniteDuration">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Duration"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Duration without a fixed end date.</skos:definition>
        <skos:prefLabel xml:lang="en">Indefinite duration</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#InnovativeProcurement -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#InnovativeProcurement">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#StrategicProcurement"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Approach whereby Buyers seek to procure an innovative work, supply or service. Additional Information: âinnovationâ means the implementation of a new or significantly improved product, service or process, including but not limited to production, building or construction processes, a new marketing method, or a new organizational method in business practices, workplace Organisation or external relations inter alia with the purpose of helping to solve societal challenges or to support the Europe 2020 strategy for smart, sustainable and inclusive growth; See: Directive 2014/24 Articles: 2, 26 (3), 31, 67 (2.a)</skos:definition>
        <skos:prefLabel xml:lang="en">Innovative procurement</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#JuryMember -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#JuryMember">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AcquiringParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Role of an Agent who evaluates a design contest.</skos:definition>
        <skos:prefLabel xml:lang="en">Jury member</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#LeadBuyer -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#LeadBuyer">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Buyer"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent who is a Buyer and takes the administrative lead of the Procedure.</skos:definition>
        <skos:prefLabel xml:lang="en">Lead buyer</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Lot -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Lot">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ProcurementObject"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A qualitative, quantitative or strategic subdivision of the goods, services or works to be procured, allowing the award of one or more Contracts.</skos:definition>
        <skos:prefLabel xml:lang="en">Lot</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#LotAwardOutcome -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#LotAwardOutcome">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AwardOutcome"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Result concerning the Lot that was attributed by the Awarder.</skos:definition>
        <skos:prefLabel xml:lang="en">Lot award outcome</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#LotGroup -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#LotGroup">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Combination of several Lots to conduct comparative assessment of the Tenders. Additional Information: The comparative assessment may refer to the Selection Criteria, Award and Value that apply to several Lots. Pending of discussion with eForms Member States may provide that, where more than one lot may be awarded to the same tenderer, contracting authorities may award contracts combining several or all lots where they have specified in the contract notice or in the invitation to confirm interest that they reserve the possibility of doing so and indicate the lots or groups of lots that may be combined.</skos:definition>
        <skos:prefLabel xml:lang="en">Lot group</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#LotGroupAwardInformation -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#LotGroupAwardInformation">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContextDescription"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Award information related to a given Group of Lots.</skos:definition>
        <skos:prefLabel xml:lang="en">Lot group award information</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#LotSpecificTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#LotSpecificTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Term"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Gathering class for conditions and stipulations related to a Lot.</skos:definition>
        <skos:prefLabel xml:lang="en">Lot specific term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Mediator -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Mediator">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AcquiringParty"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent that attempts to resolve a dispute between different Agents and come to an agreement.</skos:definition>
        <skos:prefLabel xml:lang="en">Mediator</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#MiniCompetitionAwardOutcome -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#MiniCompetitionAwardOutcome">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AwardOutcome"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Result concerning the mini-competition that was attributed by the Awarder. Additional Information: The mini-competition is the process where multiple winners or candidates of previous stages of a procedure bid for a specific procurement. It is typically used in framework agreements where the suppliers have already been pre-selected, and the mini competition is used to determine which supplier is best suited for a particular project or contract. It is also used in a Dynamic Purchasing System where the suppliers come from a list of Candidates.</skos:definition>
        <skos:prefLabel xml:lang="en">Mini competition award outcome</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#MonetaryValue -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#MonetaryValue">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A number of monetary units specified using a given unit of currency. Additional information: In the pre-award phase of the procurement, all monetary values are considered to exclude VAT based on the Directive 2014/24/EU.</skos:definition>
        <skos:prefLabel xml:lang="en">Monetary value</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#MultipleStageProcedureTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#MultipleStageProcedureTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#LotSpecificTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations defining particularities of Procedures carried out in several steps Additional Information: Generally this refers to Procedures where selection is carried out to qualify Tenderers who are then requested to submit the rest of their Tender for evaluation is Restricted Procedure.</skos:definition>
        <skos:prefLabel xml:lang="en">Multiple stage procedure term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#NonDisclosureAgreementTerm -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#NonDisclosureAgreementTerm">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#LotSpecificTerm"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Conditions and stipulations about a non-disclosure agreement. Additional information: A non-disclosure agreement is a Contract between different parties which ensures information is not shared with other parties.</skos:definition>
        <skos:prefLabel xml:lang="en">Non disclosure agreement term</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#NonPublishedInformation -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#NonPublishedInformation">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContextDescription"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Information about fields not published in this Notice. Additional Information: The non-published information may become available at a later date and may differ from one element to another within a given Document. Examples of fields that may not be immediately published are Winner, Tender and Procedure Lot Result, etc., e.g. for security reasons.</skos:definition>
        <skos:prefLabel xml:lang="en">Non published information</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#Notice -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#Notice">
        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Document"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Document published by the Buyer about market opportunities and results.</skos:definition>
        <skos:prefLabel xml:lang="en">Notice</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#NoticeAwardInformation -->


