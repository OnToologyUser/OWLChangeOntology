        <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Contract"/>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Contract resulting from using a Dynamic Purchasing System Technique or Framework Agreement Technique.</skos:definition>
        <skos:prefLabel xml:lang="en">Purchase contract</skos:prefLabel>
    </owl:Class>
    


