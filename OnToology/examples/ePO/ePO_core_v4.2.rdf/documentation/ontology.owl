        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">supports requirement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/supportsValue -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/supportsValue">
        <rdfs:domain rdf:resource="http://data.europa.eu/m8g/Evidence"/>
        <rdfs:range rdf:resource="http://data.europa.eu/m8g/SupportedValue"/>
        <rdfs:comment xml:lang="en">Supported Value that the Evidence contains.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">supports value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/thematicArea -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/thematicArea">
        <rdfs:comment xml:lang="en">The Thematic Area of a Public Service.</rdfs:comment>
        <rdfs:label xml:lang="en">thematic area</rdfs:label>
        <skos:scopeNote xml:lang="en">For example: social protection, health, recreation, culture and religion, family, traveling economic affairs, tax, staff, environment, etc.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/validityPeriod -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/validityPeriod">
        <rdfs:domain rdf:resource="http://data.europa.eu/m8g/Evidence"/>
