        <skos:definition xml:lang="en">The Selection Criteria are present in the Procurement Documents. (epo:SelectionCriteriaSummary)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 02/05/2024</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has selection criteria stated in procurement documents</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasServiceReservedToParticularProfession -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasServiceReservedToParticularProfession">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Execution of the contract is limited to a particular profession. (epo:ProfessionalSuitabilitySummary)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 02/05/2024</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has service reserved to particular profession</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasStrategicProcurementDescription -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasStrategicProcurementDescription">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Self-explanatory text about a concept. (epo:StrategicProcurement)</skos:definition>
        <skos:prefLabel xml:lang="en">Has strategic procurement description</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasSubcontractingInvolved -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasSubcontractingInvolved">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">List of Subcontractors and the subject matter they cover are required. Additional Information: The tenderer will ned to supply this information in the tender. (epo:SubcontractTerm)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 28/02/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has subcontracting involved</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasSubcontractorsProposedAboveObligation -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasSubcontractorsProposedAboveObligation">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The maximum percentage of the contract value that the contractor must subcontract. Additional information: This is used for the competitive procedure described in Title III of Directive 2009/81/EC. (epo:SubcontractTerm)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 09/11/2021</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has subcontractors proposed above obligation</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasSubjectMatter -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasSubjectMatter">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Description of the share of the contract that is to be subcontracted. Additional infromation: This can be an aggregate of several subcontracts. (epo:SubcontractingEstimate)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 09/11/2021</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has subject matter</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasSubmissionURL -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasSubmissionURL">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:editorialNote xml:lang="en">Used by eForms BT-18, eForms BT-509</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Has submission u r l</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasSuccessiveReduction -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasSuccessiveReduction">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The number of solutions or tenders will be reduced in iterative evaluations for multiple staged procedures. Additional information: This refers to multiple-stage procedures (included two-stage procedures, at least). Open Procedures can be seen as one-stage procedures. (epo:MultipleStageProcedureTerm)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 22/08/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has successive reduction</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasThresholdValue -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasThresholdValue">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The cut-off level for a given concept. Additional Information: This value is given as e.g. a minimum score, a maximum number of tenders with the highest score passing (see codelist at-voc:number-threshold). (cccev:Constraint)</skos:definition>
        <skos:prefLabel xml:lang="en">Has threshold value</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasTotalNumberOfComplainants -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasTotalNumberOfComplainants">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The number of economic operators that requested the buyer to review any of its decisions (e.g. the technical specifications, award decision), as set out in Art. 1(5) of Directive 89/665/EEC and Directive 92/13/EEC. (epo:ReviewRequestSummary)</skos:definition>
        <skos:editorialNote xml:lang="en">Used by eForms BT-712</skos:editorialNote>
        <skos:historyNote xml:lang="en">WG Approval 11/04/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has total number of complainants</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasTotalVehicles -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasTotalVehicles">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The number of all vehicles (regardless of whether clean or not) that have either been purchased, leased, rented, hired-purchased or their use has been contractually committed to for the provision of a purchased service. Additional Information: In the European Union, the legal requirements and scope for the provision of these vehicles or services are covered by Directive 2009/33/EC. (epo:VehicleInformation)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 28/07/2020</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has total vehicles</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasURL -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasURL">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The identifier of a resource. Additional Information For example: 1. The URL of the system from where to access the procurement documents; 2. The URL of the system for the submission of tender documents; 3. The URL of the system from where to download a tool to communicate with the Buyer; 4. The URL of the system used by a Technique to allow Economic Operators to exchange information with the Buyer (e.g. eAuction and DPS Systems) 5. The URL of the system used to exchange information between Buyer and EO for questions and clarifications; (cccev:Evidence) The identifier of a resource. Additional Information For example: 1. The URL of the system from where to access the procurement documents; 2. The URL of the system for the submission of tender documents; 3. The URL of the system from where to download a tool to communicate with the Buyer; 4. The URL of the system used by a Technique to allow Economic Operators to exchange information with the Buyer (e.g. eAuction and DPS Systems) 5. The URL of the system used to exchange information between Buyer and EO for questions and clarifications; (epo:Fund)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 30/09/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has u r l</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasUnitDescription -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasUnitDescription">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A narrative explanation defining the units of items being counted Additional information: This could be for example individual items or pack or two. (epo:Quantity)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 09/11/2021</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has unit description</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasUnverifiedTenders -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasUnverifiedTenders">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Offers received for which it has not been verified if they are admissible or inadmissible (e.g. because award criteria have been evaluated for all tenders and admissibility is checked only for the winning tender). (epo:SubmissionStatisticalInformation)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 28/07/2020</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has unverified tenders</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasVersion -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasVersion">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A number that identifies a specific state of a document. (epo:Document)</skos:definition>
        <skos:historyNote xml:lang="en">WG approval 18/11/2021</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has version</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasWithdrawalDate -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasWithdrawalDate">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The date and time when the request for review was withdrawn. (epo:ReviewRequest)</skos:definition>
        <skos:editorialNote xml:lang="en">Used by eForms BT-797</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Has withdrawal date</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasWithdrawalReason -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasWithdrawalReason">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The explanation for withdrawing the request for review. (epo:ReviewRequest)</skos:definition>
        <skos:editorialNote xml:lang="en">Used by eForms BT-798</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Has withdrawal reason</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasZeroEmissionVehicles -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasZeroEmissionVehicles">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The number of all zero-emission heavy-duty vehicles that have either been purchased, leased, rented, hired-purchased or their use has been contractually committed to for the provision of a purchased service. Additional Information: In the European Union, the legal requirements and scope for the provision of these vehicles or services are covered by Directive 2009/33/EC. (epo:VehicleInformation)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 28/07/2020</skos:historyNote>
        <skos:prefLabel xml:lang="en">Has zero emission vehicles</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#indicatesPerformingStaffInformationRequirement -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#indicatesPerformingStaffInformationRequirement">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Obligation to indicate the names and professional qualifications of the staff assigned to performing the contract. (epo:ProcurementCriteriaSummary)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 02/05/2024</skos:historyNote>
        <skos:prefLabel xml:lang="en">Indicates performing staff information requirement</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isAccelerated -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isAccelerated">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Statement about the fact that the procedure will be reduced due to a state of urgency. Additional Information This modifies the time limit for the receipt of requests to participate or the receipt of tenders. (epo:Procedure)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 20/08/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is accelerated</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isAdvancedElectronicSignatureRequired -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isAdvancedElectronicSignatureRequired">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Advanced or qualified electronic signature or seal (as defined in Regulation (EU) No 910/2014) is required. The submitted information is required to be signed electronically. Additional Information: Signature can be defined as &quot;data in electronic form which is attached to or logically associated with other data in electronic form and which is used by the signatory to sign. For more details on the meaning and uses of electronic signature you may consult different authoritative sources, a relevant one being for instance the Regulation (EU) 910/2014 on electronic identification and trust services for electronic transactions in the internal market. (epo:SubmissionTerm)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 21/07/2020</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is advanced electronic signature required</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isAwardedByCPB -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isAwardedByCPB">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Procedure is awarded by a Central Purchasing Body. (epo:ProcedureTerm)</skos:definition>
        <skos:prefLabel xml:lang="en">Is awarded by c p b</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isCompetitionTerminated -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isCompetitionTerminated">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">No further contracts will be awarded in this procedure. Additional Information: This can be instantiated in the post award phase. PIN for Competition needs to be signaled. This field can be used even if no contracts are awarded in the contract award notice. (epo:ProcurementProcessInformation)</skos:definition>
        <skos:editorialNote xml:lang="en">Used by eForms BT-756</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Is competition terminated</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isContractingEntity -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isContractingEntity">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Role of entities, which: (a) are contracting authorities or public undertakings and which pursue one of the activities referred to in Articles 8 to 14 of the Directive 2014/25/EU. (b) when they are not contracting authorities or public undertakings, have as one of their activities any of the activities referred to in Articles 8 to 14, or any combination thereof and operate on the basis of special or exclusive rights granted by a competent authority of a Member State. Additional Information: The indicator is needed in order to discriminate between those contracts where the Contracting Entity acts as a Contracting Authority undergoing the limits and the rules of Directive 24 and those where it acts as a Contracting Entity, with certain relaxed constraints. For example, the Contracting Entities have different thresholds for the application of Directive 24 if compared with Contracting Authorities. (epo:Buyer)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 28/04/2020</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is contracting entity</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isCoveredByGPA -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isCoveredByGPA">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Specifies whether the Agreement on Government Procurement (GPA) applies. Additional information: The GPA aims to establish a multilateral framework of balanced rights and obligations relating to public contracts with a view to achieving the liberalization and expansion of world trade. (epo:ProcurementObject)</skos:definition>
        <skos:editorialNote xml:lang="en">Used by eForms BT-115</skos:editorialNote>
        <skos:historyNote xml:lang="en">WG Approval 15/10/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is covered by g p a</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isDPSTerminated -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isDPSTerminated">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">End of the Dynamic Purchase System (DPS). Additional Information: This property can be used in the contract award notice even if no contracts are awarded. (epo:ProcurementProcessInformation)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 22/11/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is d p s terminated</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isDesignContest -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isDesignContest">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A competition which enables the buyer to acquire a plan or design via a jury. Additional information: Design contests have traditionally mostly been used in the fields of town and country planning, architecture and engineering or data processing, other purposes, such as to obtain plans for financial engineering The contest may include or not the award of prizes; (epo:Procedure)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 04/02/2021</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is design contest</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isGuaranteeRequired -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isGuaranteeRequired">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The submitted information must include a financial commitment to be used in case of default. Additional Information: See the additional information provided in the definition of the &apos;Guarantee Description&apos; element. (epo:SubmissionTerm)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 21/07/2020</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is guarantee required</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isJointProcurement -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isJointProcurement">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Multiple buyers procure together within the same procedure. Addition Information: In case the joint procurement involves buyers from different countries, the national law is specified by the epo:hasCrossBorderLaw in the ProcedureTerm . (epo:Procedure)</skos:definition>
        <skos:prefLabel xml:lang="en">Is joint procurement</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isJuryDecisionBinding -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isJuryDecisionBinding">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Indicates whether the procuring entity is bound to apply the decision of the jury. (epo:DesignContestRegimeTerm)</skos:definition>
        <skos:prefLabel xml:lang="en">Is jury decision binding</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isListedCompany -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isListedCompany">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Public companies listed on a stock exchange and subject to disclosure requirements (either by stock exchange rules or through law or enforceable means), which impose requirements to ensure adequate transparency of beneficial ownership. &lt;u&gt;Additional Information:&lt;/u&gt; In eForms this indicator is used when the nationality of beneficial owner is not provided because the Economic Operator is registered in a regulated market that is subject to disclosure requirements consistent with Union law or subject to equivalent international standards which ensure adequate transparency of ownership information. (epo:Business)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 21/11/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is listed company</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isMultipleTenderSubmissionAllowed -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#isMultipleTenderSubmissionAllowed">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">Tenderers may submit more than one competing tenders. (epo:SubmissionTerm)</skos:definition>
        <skos:historyNote xml:lang="en">WG Approval 10/10/2019</skos:historyNote>
        <skos:prefLabel xml:lang="en">Is multiple tender submission allowed</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#isNonDisclosureAgreementRequired -->


