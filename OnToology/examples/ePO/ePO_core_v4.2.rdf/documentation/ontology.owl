        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">A Role of an Agent responsible for providing offline access to the Procurement Documents.</skos:definition>
        <skos:prefLabel xml:lang="en">Offline access provider</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#OpeningTerm -->


