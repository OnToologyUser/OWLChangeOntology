        <skos:definition xml:lang="en">Overview of the Procurement Criteria.</skos:definition>
        <skos:prefLabel xml:lang="en">Procurement criteria summary</skos:prefLabel>
    </owl:Class>
    


    <!-- http://data.europa.eu/a4g/ontology#ProcurementCriterion -->


    <owl:Class rdf:about="http://data.europa.eu/a4g/ontology#ProcurementCriterion">
