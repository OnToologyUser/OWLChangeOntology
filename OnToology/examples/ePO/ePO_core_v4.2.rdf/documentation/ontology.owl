        <rdfs:comment xml:lang="en">Information Concept for which a value is expected by the Requirement.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">has concept</rdfs:label>
        <skos:scopeNote xml:lang="en">Information Concepts defined for specific Requirements also represent the basis for specifying the Supported Value an Evidence should provide.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/hasContractingAuthority -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/hasContractingAuthority">
