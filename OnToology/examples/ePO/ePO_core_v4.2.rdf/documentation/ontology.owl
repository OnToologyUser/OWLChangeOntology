        <rdfs:range rdf:resource="http://data.europa.eu/m8g/InformationConcept"/>
        <rdfs:comment xml:lang="en">Information Concept providing facts found/inferred from the Evidence.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/m8g"/>
        <rdfs:label xml:lang="en">supports concept</rdfs:label>
        <skos:scopeNote xml:lang="en">Examples of Information Concepts are values found explictly in the evidence such as a birth date or information derived from the Evidence such as &quot;I am older that 18 years&quot;.</skos:scopeNote>
    </owl:ObjectProperty>
    


    <!-- http://data.europa.eu/m8g/supportsRequirement -->


    <owl:ObjectProperty rdf:about="http://data.europa.eu/m8g/supportsRequirement">
        <rdfs:domain rdf:resource="http://data.europa.eu/m8g/Evidence"/>
        <rdfs:range rdf:resource="http://data.europa.eu/m8g/Requirement"/>
