        <skos:prefLabel xml:lang="en">Has scheme</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasSchemeVersion -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasSchemeVersion">
        <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
        <skos:definition xml:lang="en">The version of the identification scheme. (adms:Identifier)</skos:definition>
        <skos:prefLabel xml:lang="en">Has scheme version</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- http://data.europa.eu/a4g/ontology#hasSelectionCriteriaStatedInProcurementDocuments -->


    <owl:DatatypeProperty rdf:about="http://data.europa.eu/a4g/ontology#hasSelectionCriteriaStatedInProcurementDocuments">
